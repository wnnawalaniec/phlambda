{
    "name": "wojciech.nawalaniec/phlambda",
    "description": "Small library providing some functional programming tools for PHP, based on Rambda (https://github.com/ramda/ramda).",
    "type": "library",
    "license": "GPL-3.0-only",
    "keywords": ["functional", "php8", "currying", "functional programming"],
    "homepage": "https://github.com/wnnawalaniec/phlambda",
    "autoload": {
        "psr-4": {
            "Wojciech\\Phlambda\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Wojciech\\Phlambda\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Wojciech Nawalaniec",
            "email": "nawalaniec.wojciech@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "infection/infection": "^0.25.5"
    },
    "scripts": {
        "test": "phpunit",
        "fullTest": [
            "phpunit",
            "infection"
        ]
    }
}
